<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="floater_about" title="Медиа браузер">
	<layout_stack name="stack1">
		<layout_panel name="nav_controls">
			<button label="Назад" name="back" />
			<button label="Вперед" name="forward" />
			<button label="Перезагр." name="reload" />
			<button label="Перейти" name="go" />
		</layout_panel>
		<layout_panel name="parcel_owner_controls">
			<button label="Отправ. текущий URL" name="assign" />
		</layout_panel>
		<layout_panel name="external_controls">
			<button label="Откр. в моем веб-браузере" name="open_browser" />
			<check_box label="Всегда открывать в моем веб-браузере"
			     name="open_always" />
			<button label="Закрыть" name="close" />
		</layout_panel>
	</layout_stack>
</floater>
