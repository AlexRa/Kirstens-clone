<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="nearby_media" title="MEDIA NEI DINTORNI">
	<string name="media_item_count_format">
		(%ld media items)
	</string>
	<string name="default_location_text">
		Sconosciuto
	</string>
	<string name="empty_item_text">
		&lt;vuoto&gt;
	</string>
	<string name="default_parcel_media_name">
		&lt;Media Non Disponibili&gt;
	</string>
	<string name="default_parcel_media_tool_tip">
		Seleziona on/off i media nel parcel
	</string>
	<panel name="nearby_media_panel">
		<panel>
			<text name="media_item_count">
				(?? media items)
			</text>
			<button name="all_nearby_media_enable_btn" tool_tip="Seleziona on/off tutti i media nei dintorni"/>
		</panel>
		<panel>
			<button name="parcel_media_enable_btn" tool_tip="Seleziona on/off i media nel parcel"/>
			<text name="parcel_media_name">
				&lt;Nessun Media Disponibile&gt;
			</text>
		</panel>
		<panel name="parcel_media_ctrls">
			<button name="parcel_media_play_btn" tool_tip="Riproduci media nel parcel"/>
			<button name="parcel_media_pause_btn" tool_tip="Pause media nel parcel"/>
			<button name="parcel_media_stop_btn" tool_tip="Stop media nel parcel"/>
		</panel>
		<panel name="parcel_audio_ctrls">
			<button name="parcel_audio_play_btn" tool_tip="Riproduci audio nel parcel"/>
			<button name="parcel_audio_pause_btn" tool_tip="Pause audio nel parcel"/>
			<button name="parcel_audio_stop_btn" tool_tip="Stop audio nel parcel"/>
		</panel>
		<panel>
			<scroll_list name="media_list">
				<scroll_list.columns label="On/Off" name="media_stop_ctrl"/>
				<scroll_list.columns label="Nome" name="media_name"/>
			</scroll_list>
		</panel>
		<panel>
			<check_box label="Permetti auto-play ai Media" name="media_auto_play_btn" tool_tip="Permetti auto-play ai Media" value="true"/>
		</panel>
	</panel>
</floater>
