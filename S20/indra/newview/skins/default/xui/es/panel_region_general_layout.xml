<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Región" name="General">
	<text name="region_text_lbl">
		Región:
	</text>
	<text name="region_text">
		desconocida
	</text>
	<text name="version_channel_text_lbl">
		Versión:
	</text>
	<text name="version_channel_text">
