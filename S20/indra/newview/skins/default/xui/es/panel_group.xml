<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Información del grupo" name="GroupInfo" title="Información del grupo">
	<panel.string name="default_needs_apply_text">
		Hay cambios sin guardar en la pestaña actual
	</panel.string>
	<panel.string name="want_apply_text">
		¿Quieres guardar estos cambios?
	</panel.string>
	<line_editor label="Escribe aquí el nombre de tu nuevo grupo" name="group_name_editor"/>
	<texture_picker name="insignia" tool_tip="Pulsa para elegir una imagen"/>
	<accordion name="groups_accordion">
		<accordion_tab name="tab_general" title="General"/>
		<accordion_tab name="tab_roles" title="Roles"/>
		<accordion_tab name="tab_notices" title="Avisos"/>
		<accordion_tab name="tab_land" title="Terreno/Bienes"/>
	</accordion>
	<button label="Cancelar" label_selected="Cancelar" name="btn_cancel"/>
	<button label="Guardar" label_selected="Guardar" name="btn_ok"/>
	<button label="Actualizar" label_selected="Actualizar" name="btn_refresh"/>
	<button label="Entrar (0 L$)" label_selected="Entrar (0 L$)" name="join_button"/>
	<button label="Detalles" label_selected="Vista detallada" name="info_button"/>
</panel>
