			Standard
		</panel.string>
		<text name="Input">
			Input
		</text>
		<text name="My volume label">
			Min lydstyrke:
		</text>
		<slider_bar initial_value="1.0" name="mic_volume_slider" tool_tip="Ændre lydstyrke med denne skyder"/>
