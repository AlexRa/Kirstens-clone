<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="right|bottom"
 height="20"
 layout="topleft"
 left="0"
 name="media_controls"
 top="21"
 width="262">
    <panel.string
     name="play_label">
        Play
    </panel.string>
    <panel.string
     name="stop_label">
        Stop
    </panel.string>
    <panel.string
     name="pause_label">
        Pause
    </panel.string>
    <panel.string
     name="default_tooltip_label">
        No Media Specified
    </panel.string>
    <panel.string
     name="media_hidden_label">
        (URL hidden by parcel owner)
    </panel.string>
    <panel.string
     name="media_icon_tooltip_web">
        This location displays content from the World Wide Web. Click the Play button to display Web content.
    </panel.string>
    <panel.string
     name="media_icon_tooltip_movie">
        This location displays Video content. Click the Play button to play the video.
    </panel.string>
    <panel.string
     name="media_play_tooltip">
        Display Web content at this location.
    </panel.string>
    <icon
     height="22"
     image_name="icn_label_music.tga"
     layout="topleft"
     left_delta="2"
     mouse_opaque="true"
     name="music_icon"
     tool_tip="Play/pause streaming music"
     top="0"
     width="20" />
    <button
     follows="left|top"
     height="20"
     image_disabled="flyout_btn_left_disabled.tga"
     image_overlay="icn_play.tga"
     image_selected="flyout_btn_left_selected.tga"
     image_unselected="flyout_btn_left.tga"
     layout="topleft"
     left_delta="20"
     name="music_play"
     tool_tip="Play/pause streaming music"
     top_delta="1"
     width="20" />
    <button
     enabled="false"
     follows="left|top"
     height="20"
     image_disabled="flyout_btn_left_disabled.tga"
     image_overlay="icn_pause.tga"
     image_selected="flyout_btn_left_selected.tga"
     image_unselected="flyout_btn_left.tga"
     layout="topleft"
     left_delta="0"
     name="music_pause"
     tool_tip="Pause streaming music"
     top_delta="0"
     width="20" />
    <button
     follows="left|top"
     height="20"
     image_disabled="flyout_btn_right_disabled.tga"
     image_overlay="icn_stop.tga"
     image_selected="flyout_btn_right_selected.tga"
     image_unselected="flyout_btn_right.tga"
     layout="topleft"
     left_delta="20"
     name="music_stop"
     tool_tip="Stop streaming music"
     top_delta="0"
     width="20" />
    <icon
     height="22"
     image_name="icn_label_media.tga"
     layout="topleft"
     left_pad="3"
     mouse_opaque="true"
     name="media_icon"
     tool_tip="Play/pause streaming music"
     top_delta="-1"
     width="20" />
    <button
     follows="left|top"
     height="20"
     image_disabled="flyout_btn_left_disabled.tga"
     image_overlay="icn_play.tga"
     image_selected="flyout_btn_left_selected.tga"
     image_unselected="flyout_btn_left.tga"
     layout="topleft"
     left_pad="1"
     name="media_play"
     tool_tip="Play/pause streaming music"
     top_delta="1"
     width="20" />
    <button
     enabled="false"
     follows="left|top"
     height="20"
     image_disabled="flyout_btn_left_disabled.tga"
     image_overlay="icn_pause.tga"
     image_selected="flyout_btn_left_selected.tga"
     image_unselected="flyout_btn_left.tga"
     layout="topleft"
     left_delta="0"
     name="media_pause"
     tool_tip="Pause streaming media"
     top_delta="0"
     width="20" />
    <button
     follows="left|top"
     height="20"
     image_disabled="flyout_btn_right_disabled.tga"
     image_overlay="icn_stop.tga"
     image_selected="flyout_btn_right_selected.tga"
     image_unselected="flyout_btn_right.tga"
     layout="topleft"
     left_delta="20"
     name="media_stop"
     tool_tip="Stop streaming media"
     top_delta="0"
     width="20" />
    <slider_bar
     control_name="AudioLevelMaster"
     height="22"
     increment="0.05"
     initial_value="0.5"
     layout="topleft"
     left_delta="15"
     name="volume_slider"
     tool_tip="Master volume"
     top_delta="0"
     width="56" />
    <button
     control_name="MuteAudio"
     height="20"
     image_selected="icn_speaker-muted_dark.tga"
     image_unselected="icn_speaker_dark.tga"
     is_toggle="true"
     layout="topleft"
     left_delta="56"
     name="mute_master"
     scale_image="false" 
     tool_tip="Master mute"
     top_delta="-2"
     width="24" />
    <button
     control_name="ShowVolumeSettingsPopup"
     height="22"
     is_toggle="true"
     layout="topleft"
     left_delta="19"
     name="expand"
     tool_tip="Adjust individual volume levels"
     top_delta="1"
     width="22" />
</panel>
