			   show_bar="false"
			   show_mean="false" >
			</stat_bar>

			<stat_view
			   name="physicsdetail"
			   label="Physics Details"
			   show_label="true"
			   display_children="false">
			  <stat_bar
				 name="physicspinnedtasks"
