<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 height="260"
 layout="topleft"
 name="floater_media_remote"
 help_topic="floater_media_remote"
 title="MEDIAREMOTE"
 width="220">
    <panel
    follows="right|bottom"
    height="235"
    layout="topleft"
    left="0"
    name="media_remote"
    top="26"
    use_bounding_rect="true"
    width="220">
        <panel
        filename="panel_bg_tab.xml"
        height="235"
        layout="topleft"
        left="0"
        top="-1"
        width="220" />
        <panel
        class="panel_preference"
        bottom="163"
        filename="panel_audio.xml"
        height="160"
        label="Audio &amp; Video"
        layout="topleft"
        name="Volume Panel"
        width="220" />
        <panel
        filename="panel_media_controls.xml"
        height="20"
        layout="topleft"
        left="0"
        top="162"
        width="220" />
        <button
        height="20"
        image_overlay="icn_voice-call-end.tga"
        layout="topleft"
        left="4"
        name="end_call_btn"
        tool_tip="Click to hang up"
        top_delta="25"
        width="20" />
        <button
        height="22"
        image_disabled="icn_rounded-text-field.tga"
        image_disabled_selected="icn_rounded-text-field.tga"
        image_hover_selected="icn_rounded-text-field.tga"
        image_hover_unselected="icn_rounded-text-field.tga"
        image_selected="icn_rounded-text-field.tga"
        image_unselected="icn_rounded-text-field.tga"
        layout="topleft"
        left_pad="3"
        name="voice_channel_bg"
        top_delta="-1"
        width="102">
            <button.commit_callback
            function="Floater.Show"
            parameter="communicate" />
        </button>
        <panel
        filename="panel_voice_controls.xml"
        height="20"
        layout="topleft"
        left="1"
        top="210"
        width="130" />
        </panel>
</floater>
