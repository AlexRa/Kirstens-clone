<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<expandable_text
 max_height="300" >
 <textbox
  allow_html="true"
  allow_scroll="true"
  bg_visible="false"
  more_label="More" 
  follows="left|top|right"
  name="text"
  read_only="true"
  use_ellipses="true"
  word_wrap="true"
  tab_stop="true"
  v_pad="3"
  h_pad="4" >
 </textbox>
 <scroll
  name="scroll"
  follows="all"
 />
</expandable_text>
