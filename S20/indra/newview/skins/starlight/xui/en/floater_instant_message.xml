<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 border="true"
 can_resize="true"
 default_tab_group="1"
 follows="left|top|right|bottom"
 height="297"
 label="(unknown)"
 layout="topleft"
 min_height="155"
 min_width="345"
 name="im_floater"
 help_topic="im_floater"
 title="(UNKNOWN)"
 width="501">
    <floater.string
     name="ringing">
        Calling...
    </floater.string>
    <floater.string
     name="answering">
        Connecting...
    </floater.string>
    <floater.string
     name="connected">
        Connected, click End Call to hang up
    </floater.string>
    <floater.string
     name="hang_up">
        Call ended
    </floater.string>
    <floater.string
     name="voice_icon">
        icn_voice-pvtfocus.tga
    </floater.string>
    <floater.string
     name="title_string">
        Instant Message with [NAME]
    </floater.string>
    <floater.string
     name="typing_start_string">
        [NAME] is typing...
    </floater.string>
    <floater.string
     name="session_start_string">
        Starting session with [NAME] please wait.
    </floater.string>
    <floater.string
     name="default_text_label">
        Click here to instant message.
    </floater.string>
    <floater.string
     name="unavailable_text_label">
        Text chat is not available for this call.
    </floater.string>
    <floater.string
     name="live_help_dialog">
        *** Welcome to Help Request *** 
Please first check our SL Help Pages by pressing F1, or by accessing the Knowledge Base http://secondlife.com/knowledgebase/ 
If your answer is not there, please enter your question to begin, then allow a few moments for available helpers to respond.
-=-=- Response times will vary, especially during peak times -=-=-
    </floater.string>
    <button
     follows="left|top"
     height="20"
     label="Profile..."
     layout="topleft"
     left="5"
     name="profile_callee_btn"
     top="20"
     width="80" />
    <button
     follows="left|top"
     height="20"
     image_overlay="icn_voice-call-start.tga"
     image_overlay_alignment="left"
     label="Call"
     layout="topleft"
     left_pad="5"
     name="start_call_btn"
     top_delta="0"
     width="100" />
    <button
     follows="left|top"
     height="20"
     image_overlay="icn_voice-call-end.tga"
     image_overlay_alignment="left"
     label="End Call"
     layout="topleft"
     left_delta="0"
     name="end_call_btn"
     top_delta="0"
     visible="false"
     width="100" />
    <panel
     follows="left|top|right"
     height="20"
     layout="topleft"
     left_delta="96"
     name="speaker_controls"
     top_delta="-3"
     width="100">
        <slider_bar
         follows="left|bottom"
         height="15"
         increment="0.05"
         initial_value="0.5"
         layout="topleft"
         left_delta="0"
         name="speaker_volume"
         top="5"
         width="56" />
        <button
         height="20"
         image_selected="icn_speaker-muted_dark.tga"
         image_unselected="icn_speaker_dark.tga"
         layout="topleft"
         left_delta="56"
         name="mute_btn"
         tool_tip="Block voice"
         top="0"
         width="25" />
    </panel>
    <text_editor
     bg_readonly_color="ChatHistoryBgColor"
     bg_writeable_color="ChatHistoryBgColor"
     follows="left|top|right|bottom"
     font="SansSerif"
     height="221"
     layout="topleft"
     left="5"
     max_length="2147483647"
     name="im_history"
     parse_highlights="true"
     allow_html="true" 
     read_only="true"
     text_color="ChatHistoryTextColor"
     text_readonly_color="ChatHistoryTextColor"
     top="46"
     track_bottom="true"
     width="487"
     word_wrap="true" />
    <line_editor
     border_style="line"
     border_thickness="1"
     follows="left|right|bottom"
     font="SansSerif"
     height="20"
     label="Click here to instant message"
     layout="topleft"
     left_delta="0"
     max_length="1022"
     name="chat_editor"
     tab_group="1"
     top_pad="3"
     width="423" />
    <button
     follows="right|bottom"
     height="20"
     label="Send"
     layout="topleft"
     left_pad="5"
     name="send_btn"
     top_delta="0"
     width="60" />
</floater>
