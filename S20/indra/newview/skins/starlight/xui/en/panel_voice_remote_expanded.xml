<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="right|bottom"
 height="45"
 layout="topleft"
 left="0"
 name="voice_remote"
 top="45"
 use_bounding_rect="true"
 width="130">
    <panel
     filename="panel_bg_tab.xml"
     height="47"
     layout="topleft"
     left="0"
     name="panel_bg_tab"
     top="-3"
     width="130" />
    <button
     height="20"
     image_overlay="icn_voice-call-end.tga"
     layout="topleft"
     left="4"
     name="end_call_btn"
     tool_tip="Click to hang up"
     top="0"
     width="20" />
    <button
     height="22"
     image_disabled="TextField_Search_Disabled"
     image_disabled_selected="TextField_Disabled_Selected"
     image_hover_selected="TextField_Search_Active"
     image_hover_unselected="TextField_Search_Active"
     image_selected="TextField_Search_Active"
     image_unselected="TextField_Search_Off"
     layout="topleft"
     left_pad="3"
     name="voice_channel_bg"
     top_delta="-1"
     width="102">
        <button.commit_callback
         function="Floater.Show"
         parameter="communicate" />
    </button>
    <icon
     height="16"
     image_name="Unread_IM"
     layout="topleft"
     left_delta="4"
     name="voice_channel_icon"
     top_delta="3"
     width="16" />
    <text
     height="16"
     layout="topleft"
     left_pad="2"
     mouse_opaque="false"
     name="channel_label"
     text_color="0 0 0 1"
     top_delta="1"
     use_ellipses="true"
     width="65" />
    <panel
     filename="panel_voice_controls.xml"
     height="20"
     layout="topleft"
     left="1"
     top="22"
     width="130" />
</panel>
