             border_style="line"
             border_thickness="1"
             follows="left|right|bottom"
             font="SansSerif"
             height="20"
             label="Click here to instant message"
             layout="topleft"
             left_delta="0"
             max_length="1022"
             name="chat_editor"
             tab_group="1"
             top_pad="3"
             width="426" />
            <button
             follows="right|bottom"
             height="20"
             label="Send"
             layout="topleft"
             left_pad="5"
             name="send_btn"
             top_delta="0"
             width="60" />
