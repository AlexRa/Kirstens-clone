             enabled="false"
             follows="left|top"
             height="20"
             image_overlay="icn_voice-call-start.tga"
             image_overlay_alignment="left"
             label="Call"
             layout="topleft"
             left="5"
             name="start_call_btn"
             top="0"
             width="80" />
            <button
             follows="left|top"
             height="20"
             image_overlay="icn_voice-call-end.tga"
