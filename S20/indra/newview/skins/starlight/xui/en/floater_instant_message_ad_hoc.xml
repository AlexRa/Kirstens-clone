        <layout_panel
         auto_resize="false"
         filename="panel_speaker_controls.xml"
         height="120"
         layout="topleft"
         left_delta="0"
         min_width="140"
         name="active_speakers_panel"
         top_delta="175"
         visible="false"
         width="140" />
