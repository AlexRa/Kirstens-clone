		<combo_item name="CurrentWindow">
			Huidig venster
		</combo_item>
		<combo_item name="Small(128x128)">
			Klein (128x128)
		</combo_item>
		<combo_item name="Medium(256x256)">
			Middel (256x256)
		</combo_item>
		<combo_item name="Large(512x512)">
			Groot (512x512)
		</combo_item>
		<combo_item name="Custom">
			Aangepast
		</combo_item>
	</combo_box>
	<combo_box label="Resolutie" name="local_size_combo">
		<combo_item name="CurrentWindow">
			Huidig venster
		</combo_item>
		<combo_item name="320x240">
			320x240
		</combo_item>
		<combo_item name="640x480">
			640x480
		</combo_item>
		<combo_item name="800x600">
			800x600
		</combo_item>
		<combo_item name="1024x768">
			1024x768
		</combo_item>
		<combo_item name="1280x1024">
			1280x1024
		</combo_item>
		<combo_item name="1600x1200">
			1600x1200
		</combo_item>
		<combo_item name="Custom">
			Aangepast
		</combo_item>
	</combo_box>
