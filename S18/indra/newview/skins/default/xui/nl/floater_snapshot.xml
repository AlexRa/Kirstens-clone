		<combo_item name="PNG">
			PNG
		</combo_item>
		<combo_item name="JPEG">
			JPEG
		</combo_item>
		<combo_item name="BMP">
			BMP
		</combo_item>
	</combo_box>
	<spinner label="Breedte" name="snapshot_width"/>
	<spinner label="Hoogte" name="snapshot_height"/>
