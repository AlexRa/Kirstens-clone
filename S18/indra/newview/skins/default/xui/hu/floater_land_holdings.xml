		<column label="Név" name="name" />
		<column label="Helyszín" name="location" />
		<column label="Terület" name="area" />
		<column label="" name="hidden" />
	</scroll_list>
	<button label="Teleportálás" label_selected="Teleportálás" name="Teleport"
	     tool_tip="Teleportálás ennek a földnek a középpontjára." />
	<button label="Térképen mutat" label_selected="Térképen mutat" name="Show on Map"
	     tool_tip="A föld megjelenítése a világtérképen." />
