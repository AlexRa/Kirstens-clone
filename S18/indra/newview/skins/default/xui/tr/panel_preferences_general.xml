<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel label="Genel" name="general_panel">
	<combo_box name="location_combobox">
		<combo_item name="MyHome">
			Evim
		</combo_item>
		<combo_item name="MyLastLocation">
			Son lokasyonum
		</combo_item>
	</combo_box>
	<check_box label="Giriş ekranında başlangıç lokasyonunu göster"
	     name="show_location_checkbox" />
	<combo_box name="fade_out_combobox">
		<combo_item name="Never">
			Hiçbir zaman
		</combo_item>
		<combo_item name="Show Temporarily">
			Geçici süreyle göster
		</combo_item>
		<combo_item name="Always">
			Her zaman
		</combo_item>
	</combo_box>
	<check_box label="Küçük karakter adları" name="small_avatar_names_checkbox" />
	<check_box label="Kendi ekranımda adımı gizle" name="show_my_name_checkbox" />
	<text type="string" length="1" name="group_titles_textbox">
		Grup başlıkları:
	</text>
	<check_box label="Tüm grup başlıklarını gizle" name="show_all_title_checkbox" />
	<check_box label="Grup başlığımı gizle" name="show_my_title_checkbox" />
	<color_swatch label="" name="effect_color_swatch"
	     tool_tip="Renk seçiciyi açmak için tıklayınız" />
	<text type="string" length="1" name="UI Size:">
		KA boyutu:
	</text>
	<check_box label="Bağımsız çözünürlük ölçeğini kullan" name="ui_auto_scale" />
	<spinner label="Dışarda zaman aşımı:" name="afk_timeout_spinner" />
	<check_box label="Küçük haritayı döndür" name="rotate_mini_map_checkbox" />
	<check_box label="Linden doları (L$) harcandığında veya alındığında bildir"
	     name="notify_money_change_checkbox" />
	<check_box label="Sistemdeki var olan renk seçiciyi kullan"
	     name="use_system_color_picker_checkbox"
	     tool_tip="Second Life içindeki entegre renk seçici yerine sistemdekini kullanır." />
	<check_box label="Arama panelini yardımcı çubukda göster" name="show_search_panel"
	     tool_tip="Gömülü arama panelini görüntüler." />
	<text type="string" length="1" name="start_location_textbox">
		Başlangıç lokasyonu:
	</text>
	<text type="string" length="1" name="show_names_textbox">
		Ad görüntüleme:
	</text>
	<text type="string" length="1" name="effects_color_textbox">
		Efekt rengi:
	</text>
	<text type="string" length="1" name="seconds_textbox">
		saniye
	</text>
	<text type="string" length="1" name="crash_report_textbox">
		Çakışma raporları:
	</text>
	<text type="string" length="1" name="language_textbox">
		Dil:
	</text>
	<text type="string" length="1" name="language_textbox2">
		(Yeniden başlatmak gerektirir)
	</text>
	<string name="region_name_prompt">
		&lt;Bölge ismi girin&gt;
	</string>
	<combo_box name="crash_behavior_combobox">
		<combo_item type="string" length="1" name="Askbeforesending">
			Göndermeden önce sor
		</combo_item>
		<combo_item type="string" length="1" name="Alwayssend">
			Her zaman gönder
		</combo_item>
		<combo_item type="string" length="1" name="Neversend">
			Hiçbir zaman gönderme
		</combo_item>
	</combo_box>
	<combo_box name="language_combobox">
		<combo_item type="string" length="1" name="System Default Language">
			Sistem dili
		</combo_item>
		<combo_item type="string" length="1" name="English">
			English (İngilizce)
		</combo_item>
		<combo_item type="string" length="1" name="Chinese">
			汉语/漢語 (Çince) - Beta
		</combo_item>
		<combo_item type="string" length="1" name="Deutsch(German)">
			Deutsch (Almanca) - Beta
		</combo_item>
		<combo_item type="string" length="1" name="French">
			Français (Fransızca) - Beta
		</combo_item>
		<combo_item type="string" length="1" name="(Japanese)">
			日本語 (Japonca) - Beta
		</combo_item>
		<combo_item type="string" length="1" name="(Korean)">
			한국어 (Korece) - Beta
		</combo_item>
		<combo_item type="string" length="1" name="Portugese">
			Portugués (Portekizce) - Beta
		</combo_item>
		<combo_item type="string" length="1" name="Spanish">
			Español (İspanyolca) - Beta
		</combo_item>
		<combo_item type="string" length="1" name="Turkish">
			Türkçe - Beta
		</combo_item>
	</combo_box>
	<check_box label="Dili objeler ile paylaş" name="language_is_public"
	     tool_tip="Bu,iç dünya objelerinin seçtiğiniz dili tanımasını sağlar." />
</panel>
